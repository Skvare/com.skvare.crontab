<?xml version="1.0"?>
<extension key="com.skvare.crontab" type="module">
  <file>crontab</file>
  <name>Crontab</name>
  <description>Crontab extension for advanced scheduling of job.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Sunil Pawar</author>
    <email>sunil@skvare.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/Skvare/com.skvare.crontab</url>
    <url desc="Documentation">https://github.com/Skvare/com.skvare.crontab</url>
    <url desc="Support">https://github.com/Skvare/com.skvare.crontab</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-10-13</releaseDate>
  <version>1.2</version>
  <compatibility>
    <ver>5.27</ver>
    <ver>5.33</ver>
    <ver>5.39</ver>
    <ver>5.51</ver>
    <ver>5.63</ver>
  </compatibility>
  <comments>Use Linux crontab-like expressions for scheduled jobs.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Crontab</namespace>
  </civix>
</extension>
